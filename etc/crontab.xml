<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="vendiro_inventory" instance="TIG\Vendiro\Cron\Inventory" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="vendiro_get_orders" instance="TIG\Vendiro\Cron\Order" method="saveOrder">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="vendiro_tracktrace" instance="TIG\Vendiro\Cron\TrackTrace" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
</config>